@{
    //display
}
@model DateTime
@{
    string userTimeZoneStr = (string)Context.Profile.GetPropertyValue("TimeZone");
    TimeZoneInfo userTimeZone = TimeZoneInfo.FindSystemTimeZoneById(userTimeZoneStr);
    var newTime = TimeZoneInfo.ConvertTimeFromUtc(Model, userTimeZone);
}
@Html.Encode(newTime)
