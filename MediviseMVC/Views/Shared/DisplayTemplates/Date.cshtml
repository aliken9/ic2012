@{
    //display
}

@model DateTime
@{
    string userTimeZoneStr = (string)Context.Profile.GetPropertyValue("TimeZone");
    TimeZoneInfo userTimeZone = TimeZoneInfo.FindSystemTimeZoneById(userTimeZoneStr);
    var modelTime = (DateTime)Model;
    var newTime = TimeZoneInfo.ConvertTimeFromUtc(modelTime, userTimeZone);
}
@Html.Encode(newTime.ToString("MM/dd/yyyy"))
